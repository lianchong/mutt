# ------ HTML Documents ---------------- #
text/html; w3m -T text/html -I `echo %{charset} | tr '[A-Z]' '[a-z]' | sed s/gb2312/gbk/g` -dump %s; nametemplate=%s.html; description="HTML Text"; copiousoutput
text/html; lynx -force_html -assume_charset=gbk -assume_local_charset=gbk -assume_unrec_charset=gbk -display_charset=utf8 -dump -force_html '%s'; description="HTML Text"; nametemplate=%s.html; copiousoutput
text/html; html2text '%s'; description="HTML Text"; copiousoutput
text/html; elinks -dump -default-mime-type text/html %s; needsterminal; copiousoutput
text/html; links -dump '%s'; copiousoutput; description="HTML Text"; nametemplate=%s.html; copiousoutput
text/html; sensible-browser '%s'; description="HTML Text"; nametemplate=%s.html; copiousoutput
text/html; firefox '%s'; description="HTML Text"; test=test -n "$DISPLAY";  nametemplate=%s.html; needsterminal
text/html; seamonkey '%s'; description="HTML Text"; test=test -n "$DISPLAY";  nametemplate=%s.html; needsterminal
text/html; swiftfox '%s'; description="HTML Text"; test=test -n "$DISPLAY";  nametemplate=%s.html; needsterminal

# ------ PDF Documents ----------------- #
application/pdf;acroread -tempFile %s; test=test -n "$DISPLAY"; nametemplate=%s.pdf; needsterminal
#application/pdf; pdftohtml -q -stdout %s | w3m -T text/html -I `echo %{charset} | tr '[A-Z]' '[a-z]' | sed s/gb2312/gbk/g` -dump -T text/html %s ; nametemplate=%s.pdf; copiousoutput
application/pdf; gv '%s'; test=test -n "$DISPLAY"; needsterminal
application/pdf; evince '%s'; test=test -n "$DISPLAY"; nametemplate=%s.pd; needsterminal
application/x-pdf; acroread -tempFile '%s'; test=test -n "$DISPLAY"; nametemplate=%s.pdf; needsterminal
application/x-pdf; evince '%s'; test=test -n "$DISPLAY"; nametemplate=%s.pdf; needsterminal
application/x-bzpdf; evince '%s'; test=test -n "$DISPLAY"; nametemplate=%s.pdf.gz; needsterminal
application/x-gzpdf; evince '%s'; test=test -n "$DISPLAY"; nametemplate=%s.pdf.bz2; needsterminal

# ------ Text Documents ---------------- #
text/plain; less -isr '%s'; copiousoutput
text/plain; most '%s'; copiousoutput
text/plain; more '%s'; copiousoutput
text/plain; view '%s'; edit=vim '%s'; compose=vim '%s'; needsterminal
text/plain; gview -f '%s'; edit=gvim -f '%s'; compose=gvim -f '%s'; test=test "$DISPLAY" != ""
text/plain; soffice -writer '%s'; edit=soffice -writer '%s'; test=test -n "$DISPLAY"; description="Plain Text Document"; nametemplate=%s.txt
text/plain; shownonascii iso-8859-1 '%s'; description="Plain ASCII Text"; test=test "$(echo %{charset} | tr "[A-Z]" "[a-z]")" = iso-8859-1 -a "$DISPLAY" != ""
text/*; less '%s'; copiousoutput
text/*; more '%s'; copiousoutput
text/*; most '%s'; copiousoutput
text/*; view '%s'; edit=vim '%s'; compose=vim '%s'; needsterminal
text/*; gview -f '%s'; edit=gvim -f '%s'; compose=gvim -f '%s'; test=test "$DISPLAY" != ""; needsterminal

# -------------------------------------- #
# -- Key files ------------------------- #
application/x-pkcs7-signature;openssl pkcs7 -in %s -inform der -noout -print_certs -text | less -isr; copiousoutput

# -------------------------------------- #
# -- Image files ----------------------- #
# -------------------------------------- #
image/jpg; display -size 640x480 'jpg:%s'; test=test -n "$DISPLAY"; needsterminal
image/jpg; xli -quiet '%s'; test=test -n "$DISPLAY"; description="JPEG Image"; nametemplate=%s.jpg; needsterminal
image/jpg; seamonkey '%s'; description="JPEG Image"; test=test -n "$DISPLAY";  nametemplate=%s.jpg; needsterminal
image/jpg; swiftfox '%s'; description="JPEG Image"; test=test -n "$DISPLAY";  nametemplate=%s.jpg; needsterminal
