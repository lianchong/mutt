# ------ Microsoft Documents ----------- #
application/msword; soffice -writer '%s'; edit=soffice -writer '%s'; test=test -n "$DISPLAY"; description="Microsoft Word Document"; nametemplate=%s.doc; needsterminal
application/msword;wvHtml --charset=gbk %s - | w3m -dump -I gbk -T text/html ; description="Microsoft Word Document"; copiousoutput
application/vnd.ms-word.document.macroEnabled.12; soffice -writer '%s'; edit=soffice -writer '%s'; test=test -n "$DISPLAY"; description="Office Open XML Document with Macros Enabled"; nametemplate=%s.docm; needsterminal
